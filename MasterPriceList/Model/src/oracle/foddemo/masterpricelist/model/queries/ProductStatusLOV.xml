<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProductStatusLOV"
  Version="11.1.1.54.68"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="LookupCodesEO.LOOKUP_CODE, 
       LookupCodesEO.LOOKUP_TYPE, 
       LookupCodesEO.LANGUAGE, 
       LookupCodesEO.MEANING"
  FromList="LOOKUP_CODES LookupCodesEO"
  Where="((LookupCodesEO.LOOKUP_TYPE = 'PRODUCT_STATUS_CODE') AND (LookupCodesEO.LANGUAGE = :sessionLanguage ))"
  OrderBy="LookupCodesEO.MEANING">
  <Variable
    Name="sessionLanguage"
    Kind="where"
    Type="java.lang.String"
    IsUpdateable="false">
    <TransientExpression><![CDATA[adf.userSession.userData.CurrentSessionLanguage]]></TransientExpression>
  </Variable>
  <EntityUsage
    Name="LookupCodesEO"
    Entity="oracle.foddemo.masterpricelist.model.entities.LookupCodesEO"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="LookupCode"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LookupCode"
    EntityUsage="LookupCodesEO"
    AliasName="LOOKUP_CODE"/>
  <ViewAttribute
    Name="LookupType"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="LookupType"
    EntityUsage="LookupCodesEO"
    AliasName="LOOKUP_TYPE"/>
  <ViewAttribute
    Name="Language"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Language"
    EntityUsage="LookupCodesEO"
    AliasName="LANGUAGE"/>
  <ViewAttribute
    Name="Meaning"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Meaning"
    EntityUsage="LookupCodesEO"
    AliasName="MEANING"/>
</ViewObject>

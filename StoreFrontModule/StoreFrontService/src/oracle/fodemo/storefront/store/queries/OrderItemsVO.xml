  <SQLQuery>
    <![CDATA[SELECT OrderItemEO.ORDER_ID, 
       OrderItemEO.LINE_ITEM_ID, 
       OrderItemEO.PRODUCT_ID, 
       OrderItemEO.QUANTITY, 
       OrderItemEO.UNIT_PRICE, 
       ProductBaseEO.PRODUCT_ID AS PRODUCT_ID1, 
       ProductBaseEO.SUPPLIER_ID, 
       ProductBaseEO.CATEGORY_ID, 
       ProductBaseEO.PRODUCT_NAME, 
       ProductBaseEO.COST_PRICE, 
       ProductBaseEO.LIST_PRICE, 
       ProductBaseEO.MIN_PRICE, 
       ProductBaseEO.WARRANTY_PERIOD_MONTHS, 
       ProductBaseEO.SHIPPING_CLASS_CODE, 
       ProductTranslationEO.PRODUCT_ID AS PRODUCT_ID2, 
       ProductTranslationEO.LANGUAGE, 
       ProductTranslationEO.SOURCE_LANG, 
       ProductTranslationEO.DESCRIPTION, 
       ProductTranslationEO.ADDITIONAL_INFO, 
       ProductTranslationEO.CREATED_BY AS CREATED_BY2, 
       ProductTranslationEO.CREATION_DATE AS CREATION_DATE2, 
       ProductTranslationEO.LAST_UPDATED_BY AS LAST_UPDATED_BY2, 
       ProductTranslationEO.LAST_UPDATE_DATE AS LAST_UPDATE_DATE2, 
       ProductTranslationEO.OBJECT_VERSION_ID AS OBJECT_VERSION_ID2, 
       CategoryTranslationEO.CATEGORY_ID AS CATEGORY_ID1, 
       CategoryTranslationEO.CATEGORY_NAME, 
       CategoryTranslationEO.CATEGORY_DESCRIPTION, 
       CategoryTranslationEO.LANGUAGE AS LANGUAGE1,
       decode(ProductBaseEO.shipping_class_code, 'CLASS1',  ShippingOptionBaseEO.cost_per_class1_item,
                                     'CLASS2', ShippingOptionBaseEO.cost_per_class2_item,
                                     'CLASS3', ShippingOptionBaseEO.cost_per_class3_item) as SHIPPING_COST
FROM 
ORDERS OrderEO,
ORDER_ITEMS OrderItemEO, 
PRODUCTS_BASE ProductBaseEO, 
PRODUCT_TRANSLATIONS ProductTranslationEO, 
CATEGORY_TRANSLATIONS CategoryTranslationEO, 
SHIPPING_OPTIONS_BASE ShippingOptionBaseEO
WHERE 
OrderItemEO.PRODUCT_ID = ProductBaseEO.PRODUCT_ID AND 
ProductTranslationEO.PRODUCT_ID  = ProductBaseEO.PRODUCT_ID AND 
CategoryTranslationEO.CATEGORY_ID = ProductBaseEO.CATEGORY_ID AND 
OrderEO.ORDER_ID = OrderItemEO.ORDER_ID AND
ShippingOptionBaseEO.SHIPPING_OPTION_ID = OrderEO.SHIPPING_OPTION_ID AND ProductTranslationEO.LANGUAGE = USERENV('CLIENT_INFO') AND
CategoryTranslationEO.LANGUAGE=USERENV('CLIENT_INFO')]]>
  </SQLQuery>

<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProductOrdersCountVO"
  Version="11.1.1.53.5"
  BindingStyle="OracleName"
  CustomQuery="true"
  FetchMode="FETCH_AS_NEEDED"
  FetchSize="25"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <SQLQuery>
    <![CDATA[SELECT  OrderItems.product_id,
        ProductsBase.PRODUCT_NAME,
        ProductsBase.LIST_PRICE,
       SUM(OrderItems.quantity) as Items_Ordered 
FROM order_items OrderItems, products_base ProductsBase
WHERE 
ProductsBase.product_id = OrderItems.product_id
GROUP By 
OrderItems.product_id, 
ProductsBase.Product_Name, 
ProductsBase.category_id, 
ProductsBase.LIST_PRICE
ORDER BY Items_Ordered DESC]]>
  </SQLQuery>
  <ViewAttribute
    Name="ProductId"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="PRODUCT_ID"
    Expression="PRODUCT_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ProductName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRODUCT_NAME"
    Expression="PRODUCT_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ListPrice"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="8"
    Scale="2"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="LIST_PRICE"
    Expression="LIST_PRICE"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Description"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DESCRIPTION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4000"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ItemsOrdered"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ITEMS_ORDERED"
    Expression="ITEMS_ORDERED"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="ProductId"/>
  </AttrArray>
</ViewObject>

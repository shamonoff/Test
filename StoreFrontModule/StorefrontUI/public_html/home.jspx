                              </af:panelLabelAndMessage>
                            </af:showDetail>
                          </af:column>
                          <af:column width="150" id="c6">
                            <af:panelFormLayout id="pfl5">
                              <af:panelLabelAndMessage label="#{res['global.cart.quantity']}"
                                                       id="plam14">
                                <af:inputNumberSpinbox autoSubmit="true"
                                                       id="quantity" columns="3"
                                                       value="#{row.Quantity}"
                                                       readOnly="#{row != shoppingCartBean.selectedRow}"
                                                       valueChangeListener="#{shoppingCartBean.quanityValueChangeListener}"
                                                       minimum="1">
                                  <f:validator binding="#{row.bindings.Quantity.validator}"/>
                                  <af:convertNumber groupingUsed="false"
                                                    pattern="#{bindings.MyShoppingCart.hints.Quantity.format}"/>
                                </af:inputNumberSpinbox>
                              </af:panelLabelAndMessage>
                            </af:panelFormLayout>
                          </af:column>
                        </af:table>
                      </af:panelHeader>

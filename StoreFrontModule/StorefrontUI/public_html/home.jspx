                                          partialTriggers="cartTab"
                                          partialSubmit="true"
                                          actionListener="#{shoppingCartBean.removeAllItems}"/>
                      </af:panelGroupLayout>
                    </f:facet>
                  </af:panelStretchLayout>
              </f:facet>
            </af:panelStretchLayout>

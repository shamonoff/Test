                                           id="pgl16">
                        <af:outputText inlineStyle="font-weight:bold;white-space:nowrap;"
                                       value="#{res['home.cart.subtotal']}="
                                       id="ot29"/>
                        <af:spacer width="5px" id="s8"/>
                        <af:outputText id="estimatedTotalInPopup"
                                       inlineStyle="color:#96171a;font-weight:bold;"
                                       partialTriggers="cartTab:quantity removeFromCart removeAll"
                                       value="#{bindings.LineItemsSubtotal}"/>
                      </af:panelGroupLayout>
                    </f:facet>
                    <f:facet name="top">
                      <af:panelGroupLayout layout="horizontal" halign="right"
                                           id="pgl17">
                        <af:commandButton id="removeFromCart"
                                          text="#{res['global.cart.remove']}"
                                          partialSubmit="true"

                       ApplicationName="devguide.advanced.customprops.ProductModule"
                       DeployPlatform="LOCAL" JDBCName="FOD"
                       jbo.project="devguide.advanced.customprops.ProgrammaticallySetProperties">
         <Security AppModuleJndiName="devguide.advanced.customprops.ProductModule"/>
      </AppModuleConfig>
      <AppModuleConfig name="ProductModuleShared"
                       ApplicationName="devguide.advanced.customprops.ProductModule"

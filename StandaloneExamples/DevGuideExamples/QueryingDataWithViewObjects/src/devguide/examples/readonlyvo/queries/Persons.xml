    <![CDATA[select PERSON_ID, EMAIL, FIRST_NAME, LAST_NAME, PERSON_TYPE_CODE, PRIMARY_ADDRESS_ID, SUBSTR(FIRST_NAME,1,1)||'. '||LAST_NAME AS SHORT_NAME
from PERSONS 
where (upper(FIRST_NAME) like upper(:TheName)||'%'
   or  upper(LAST_NAME)  like upper(:TheName)||'%')
  and PERSON_ID between :LowUserId and :HighUserId
order by EMAIL]]>
